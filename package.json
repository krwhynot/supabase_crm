{
  "name": "supabase-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:prod": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts --quiet --fix",
    "type-check": "vue-tsc --noEmit",

{
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm ci",
  "env": {
    "NODE_ENV": "production",
    "SUPABASE_ENV": "production",
    "MCP_ENABLED": "false"
  },
  "build": {
    "env": {
      "REACT_APP_SUPABASE_URL": "@supabase-url",
      "REACT_APP_SUPABASE_ANON_KEY": "@supabase-anon-key"
    }
  }
}
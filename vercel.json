{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "rewrites": [
    { "source": "/((?!assets/).*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite-supabase-url",
    "VITE_SUPABASE_ANON_KEY": "@vite-supabase-anon-key"
  },
  "build": {
    "env": {
      "VITE_SUPABASE_URL": "@vite-supabase-url",
      "VITE_SUPABASE_ANON_KEY": "@vite-supabase-anon-key"
    }
  }
}